<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.meudindin">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"

        android:supportsRtl="true"
        android:theme="@style/Theme.MeuDinDin">
        <activity
            android:name=".acitivity_listaClassificada"
            android:label="@string/title_activity_acitivity_lista_classificada"
            android:theme="@style/Theme.MeuDinDin.NoActionBar"></activity>
        <activity
            android:name=".activity_extrato"
            android:label="@string/title_activity_extrato"
            android:theme="@style/Theme.MeuDinDin.NoActionBar" />
        <activity
            android:name=".activityPesquisar"
            android:label="@string/title_activity_lista_personalisada"
            android:theme="@style/Theme.MeuDinDin.NoActionBar" />
        <activity android:name=".CadastroActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>